opagation();\r\n    });\r\n\r\n    document.addEventListener('click', function(event) {\r\n        if (!logoutMenu.contains(event.target) && event.target !== logoutMenu) {\r\n            logoutMenu.style.display = 'none';\r\n        }\r\n    });\r\n\r\n</script>\r\n\r\n</body>\r\n</html>\r\n\r\n
